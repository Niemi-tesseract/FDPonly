{{>navZX}}
<div class="container">
    <h3>List of airports</h3>
    <table class="table" class="display" id="example">
        <tr>
            <th>icao</th>
            <th>name</th>
            <th>city</th>
        </tr>
        {{#each airport_list}}
        <tr>
            <td>
                <a href="/airports/{{this.icao}}">{{this.icao}}</a>
            </td>
            <td>{{this.name}}</td>
            <td>{{this.city}}</td>
        </tr>
        {{/each}}
    </table>
</div>